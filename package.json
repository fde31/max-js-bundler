{
	"name": "max-js-bundler",
	"description": "CLI Tool to bundle and transpile modern JS for usage in [js] and [jsui] in Max MSP.",
	"version": "1.2.0",
	"author": "Florian Demmer (@fde31)",
	"bin": {
		"max-js-bundler": "./bin/run"
	},
	"bugs": "https://github.com/fde31/max-js-bundler/issues",
	"dependencies": {
		"@babel/core": "^7.10.2",
		"@babel/plugin-external-helpers": "^7.10.1",
		"@babel/plugin-transform-runtime": "^7.10.1",
		"@babel/preset-env": "^7.10.2",
		"@babel/runtime-corejs3": "^7.10.2",
		"@oclif/command": "^1.6.1",
		"@oclif/config": "^1.15.1",
		"@oclif/errors": "^1.2.2",
		"@oclif/plugin-help": "^3.1.0",
		"@rollup/plugin-babel": "^5.0.3",
		"@rollup/plugin-commonjs": "^13.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^8.0.1",
		"core-js": "^3.6.5",
		"rollup": "github:fde31/rollup#fde/rollup_update",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-node-globals": "^1.4.0",
		"tslib": "^2.0.0"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.22.2",
		"@oclif/test": "^1.2.6",
		"@types/chai": "^4.2.11",
		"@types/mocha": "^7.0.2",
		"@types/node": "^14.0.11",
		"@types/rollup-plugin-node-builtins": "^2.1.1",
		"@types/rollup-plugin-node-globals": "^1.4.0",
		"chai": "^4.2.0",
		"cross-env": "^7.0.2",
		"eslint": "^7.2.0",
		"eslint-config-c74-ts": "^2.0.0",
		"globby": "^11.0.1",
		"mocha": "^7.2.0",
		"nyc": "^15.1.0",
		"ts-node": "^8.10.2",
		"typescript": "^3.3.3333"
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"files": [
		"/bin",
		"/examples",
		"/lib",
		"/oclif.manifest.json"
	],
	"homepage": "https://github.com/fde31/max-js-bundler",
	"keywords": [
		"oclif"
	],
	"license": "MIT",
	"main": "lib/index.js",
	"oclif": {
		"commands": "./lib/commands",
		"bin": "max-js-bundler",
		"plugins": [
			"@oclif/plugin-help"
		]
	},
	"repository": "fde31/max-js-bundler",
	"scripts": {
		"lint": "eslint ./src ./test --ext .ts",
		"postpack": "rm -f oclif.manifest.json",
		"prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
		"test": "npm run lint && cross-env TS_NODE_FILES=true nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif-dev readme && git add README.md"
	},
	"types": "lib/index.d.ts"
}
